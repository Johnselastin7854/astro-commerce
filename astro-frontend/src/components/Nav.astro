---
          const props = Astro.props

---

<nav class="border-b bg-white dark:bg-zinc-800 dark:border-zinc-700">
    <div class="flex justify-end sm:justify-between p-4 max-w-screen-xl mx-auto h-[80px]">
      <!-- Left -->
      <ul class=" flex items-center gap-6 ">
        <li >
          <a href="/products/1" class="block text-zinc-900 dark:text-gray-200">Women</a>
        </li>
        <li>
          <a href="/products/2" class="block text-zinc-900 dark:text-gray-200">Men</a>
        </li>
        <li>
          <a href="/products/3" class="block text-zinc-900 dark:text-gray-200">Children</a>
        </li>
      </ul>

      <!-- Center -->
      <div class="flex items-center">
        <a href="/" class="font-semibold dark:text-white  items-center "
        > 
        <span class="flex items-center gap-2 text-red-400">
      
        <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 24 24" {...props}>
            <path fill="currentColor" d="M15.502 20.005A6.52 6.52 0 0 1 12 23.507a6.52 6.52 0 0 1-3.502-3.502h2.26c.326.488.747.912 1.242 1.243c.495-.33.916-.755 1.242-1.243zM18 14.81l2 2.268v1.927H4v-1.927l2-2.268V9.005c0-3.483 2.504-6.447 6-7.545c3.496 1.098 6 4.062 6 7.545zm-.73 2.195L16 15.565v-6.56c0-2.318-1.57-4.43-4-5.421c-2.43.99-4 3.103-4 5.42v6.561l-1.27 1.44zm-5.27-6a2 2 0 1 1 0-4a2 2 0 0 1 0 4" />
        </svg>
        Astro Commerce
      </span></a
      >
      </div>

      <!-- Right -->
      <ul class=" flex items-center gap-6">
        <li>
          <a href="/" class="block text-zinc-900 dark:text-gray-200">Home</a>
        </li>
        <li>
          <a href="/" class="block text-zinc-900 dark:text-gray-200">About</a>
        </li>
        <li>
          <a href="/" class="block text-zinc-900 dark:text-gray-200">Contact</a>
        </li>
    
        <li class=" flex gap-4  text-zinc-900 dark:text-gray-200 cursor-pointer items-center">

          
          <!-- User Icon -->
          <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 24 24" {...props}>
            <path fill="currentColor" d="M20 22h-2v-2a3 3 0 0 0-3-3H9a3 3 0 0 0-3 3v2H4v-2a5 5 0 0 1 5-5h6a5 5 0 0 1 5 5zm-8-9a6 6 0 1 1 0-12a6 6 0 0 1 0 12m0-2a4 4 0 1 0 0-8a4 4 0 0 0 0 8" />
        </svg>

          <!-- Cart Icon -->
          <div class="cartIcon relative" >
            <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 24 24" {...props}>
                <path fill="currentColor" d="M4.005 16V4h-2V2h3a1 1 0 0 1 1 1v12h12.438l2-8H8.005V5h13.72a1 1 0 0 1 .97 1.243l-2.5 10a1 1 0 0 1-.97.757H5.004a1 1 0 0 1-1-1m2 7a2 2 0 1 1 0-4a2 2 0 0 1 0 4m12 0a2 2 0 1 1 0-4a2 2 0 0 1 0 4" />
              </svg>
            <span class="absolute top-[-10px] right-[-10px] bg-blue-600 text-white text-xs w-5 h-5 flex items-center justify-center rounded-full">
              <!-- {products?.length || 8} -->
               0
            </span>
          </div>
        </li>
      </ul>
    </div>

    <!-- Cart Component -->
    <!-- {isCartOpen && (
      <div class="cart">
        <p>Cart is Open! You have {products.length} items.</p>
      </div>
    )} -->
  </nav>

  <script>
    const cartToggle = document.querySelector(".cartIcon");
    cartToggle.addEventListener("click", () => {
      document.querySelector(".cart").classList.toggle("hidden");
    });
  </script>